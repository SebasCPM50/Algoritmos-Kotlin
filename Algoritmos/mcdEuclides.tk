fun mcdEuclides(a: Int, b: Int): Int {
    var x = a
    var y = b
    
    while (y != 0) {
        val temp = y
        y = x % y
        x = temp
    }
    
    return x
}

fun main() {
    val a = 56  // primer número (ejemplo)
    val b = 98  // segundo número (ejemplo)

    val mcd = mcdEuclides(a, b)
    println("El MCD de $a y $b es: $mcd")
}